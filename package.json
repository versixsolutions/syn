{
  "name": "versix-norma",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directs --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test -- --run",
    "prepare": "husky install",
    "test:embeddings": "tsx scripts/test-embeddings.ts",
    "setup:qdrant": "tsx scripts/setup-qdrant.ts",
    "reindex:qdrant": "tsx scripts/reindex-with-embeddings.ts",
    "migrate:qdrant": "tsx scripts/migrate-to-qdrant.ts",
    "seed:assembleia": "tsx scripts/seed-assembleia.ts",
    "check:tables": "tsx scripts/check-assembleias-tables.ts",
    "check:storage": "tsx scripts/check-storage-bucket.ts",
    "check:all": "tsx scripts/check-assembleias-tables.ts && tsx scripts/check-storage-bucket.ts",
    "clean": "rm -rf node_modules dist coverage .vercel",
    "reinstall": "npm run clean && npm install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@axe-core/react": "^4.11.0",
    "@huggingface/inference": "^4.13.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/react": "^10.27.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.56.2",
    "@xenova/transformers": "^2.17.2",
    "dompurify": "^3.3.0",
    "jspdf": "^3.0.4",
    "pdfjs-dist": "^5.4.394",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^6.21.0",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.4.0",
    "zod": "^3.22.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@storybook/addon-a11y": "^10.1.2",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-themes": "^10.1.2",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/addon-vitest": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.16",
    "cypress": "^15.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^10.1.2",
    "husky": "^9.1.6",
    "jsdom": "^27.2.0",
    "lint-staged": "^15.2.10",
    "playwright": "^1.57.0",
    "postcss": "^8.4.32",
    "storybook": "^10.1.2",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^4.0.14"
  }
}
